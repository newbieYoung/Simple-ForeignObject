.mp-share{
  width:100%;
  position:absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  height:100%;
  background-color:#f4f4f4;
  box-sizing:border-box;
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
}

.mp-share .qrcode-canvas{
  position:absolute;
  top:-300px;
  left:-300px;
  width:200px;
  height:200px;
  z-index:100;
}

.mp-share .mps-content{
  max-width:560px;
  margin-left:32rpx;
  margin-right:32rpx;
  padding-top:2.83vh;
  padding-bottom:10px;
  background-color:#fff;
  border-radius:12rpx;
  margin-top:-3vh;
}

.mp-share .mps-info{
  display:flex;
  height:40px;
  align-items:center;
  padding-left:32rpx;
  padding-right:32rpx;
  margin-bottom:2.22vh;
  width:100%;
  box-sizing:border-box;
}

.mp-share .mps-info .mpsi-img{
  width:40px;
  height:40px;
  background-size:100% 100%;
  background-repeat:no-repeat;
  border-radius:40px;
  margin-right:8px;
}

.mp-share .mps-info .mpsi-text{
  flex:1;
  width:0;
}

.mp-share .mps-info .mpsi-text>view{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.mp-share .mps-info .mpsi-text>view:first-child{
  font-size:16px;
  color:#313131;
  line-height:22px;
  font-weight:700;

}

.mp-share .mps-info .mpsi-text>view:last-child{
  font-size:14px;
  color:#313131;
  opacity:0.6;
  line-height:16px;
}

.mp-share .mps-end{
  display:flex;
  align-items:center;
  margin-left:32rpx;
  margin-right:32rpx;
}

.mp-share .mps-end .mpse-qrcode{
  width:80px;
  height:80px;
  margin-right:12px;
  position:relative;
}

.mp-share .mps-end .mpse-qrcode>image{
  width:100%;
  height:100%;
}

.mp-share .mps-end .mpse-no{
  width:11px;
  height:11px;
  font-size:12px;
  color:#929292;
  border:1px solid #979797;
  display:flex;
  align-items:center;
  justify-content:center;
  border-radius:11px;
  margin-right:7rpx;
}

.mp-share .mps-end .mpse-text{
  flex:1;
}

.mp-share .mps-end .mpse-text>view{
  font-size:12px;
  color:#929292;
  display:flex;
  align-items:center;
}

.mp-share .mps-end .mpse-logo>view:first-child{
  width:46px;
  height:46px;
  background-image:url('data:image/png;base64,');
  background-size:100% 100%;
  background-repeat:no-repeat;
}

.mp-share .mp-list .mpl-item{
  margin-bottom:44rpx;
}

.mp-share .mps-btns{
  max-width:620px;
  display:flex;
  height:39px;
  padding-left:16rpx;
  padding-right:16rpx;
  box-sizing:border-box;
  width:100%;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  bottom:6vh;
}

.mp-share .mps-btns>button,
.mp-share .mps-btns>view{
  flex:1;
  margin-left:16rpx;
  margin-right:16rpx;
  height:100%;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size:17px;
  background-color:#fff;
  border-radius:6px;
  font-weight:700;
  color:#313131;
}

.mp-share .mps-btns>button:last-child,
.mp-share .mps-btns>view:last-child{
  background-image: linear-gradient(46deg, #FF393C 0%, #FF8560 100%);
  border-radius: 6px;
  color:#fff;
}

@media screen and (max-height: 670px) {
  .mp-share .mps-btns{
    position:absolute;
    left:0;
    right:0;
    bottom:3vh;
  }
  .mp-share .mp-list .mpl-item.mpl-single{
    margin-bottom:1vh;
  }
}

@media screen and (min-width: 375px) {
  .mp-share .mp-list .mpl-item{
    margin-bottom:22px;
  }
}

/* 列表 */
.mp-list{
  padding-left:32rpx;
  padding-right:32rpx;
  width:100%;
  box-sizing:border-box;
  background-color:#fff;
  display:flex;
  flex-direction:column;
}

.mp-list .mpl-item{
  display:flex;
  align-items:center;
  margin-bottom:56rpx;
  position:relative;
}

.mp-list .mpl-item.mpl-single{
  flex-direction:column;
}

.mp-list .mpl-single .mpl-img{
  width:38.23vh;
  height:38.23vh;
  max-height:496px;
  max-width:496px;
  margin-bottom:11px;
}

.mp-list .mpl-single .mpl-detail{
  width:100%;
  margin-left:0;
}

.mp-list .mpl-btn-container{
  height:100%;
  position:absolute;
  top:0;
  right:-32rpx;
  bottom:0;
  width:46rpx;
  max-width:23px;
  padding-right:32rpx;
  padding-left:20rpx;
}

.mp-list .mpl-btn{
  width:46rpx;
  max-width:23px;
  height:46rpx;
  max-height:23px;
  position:absolute;
  top:0;
  bottom:0;
  margin:auto;
  right:32rpx;
}

.mp-list .mpl-btn.mpl-add{
  background-image:url('data:image/png;base64,');
  background-size:100% 100%;
  background-repeat:no-repeat;
}

.mp-list .mpl-btn.mpl-del{
  background-image:url('data:image/png;base64,');
  background-size:100% 100%;
  background-repeat:no-repeat;
}

.mp-list .mpl-detail{
  flex:1;
  margin-left:23rpx;
}

.mp-list .mpl-tag{
  display:inline-block;
  height:32rpx;
  line-height:32rpx;
  font-size:22rpx;
  color:#FF584B;
  border:1px solid #FF584B;
  padding-left:10rpx;
  padding-right:10rpx;
  margin-right:4rpx;
  border-radius:4rpx;
}

.mp-list .mpl-title{
  font-size:28rpx;
  color:#000;
  line-height:40rpx;
  height:80rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-right:70rpx;
}

.mp-list.no-btns .mpl-title{
  margin-right:0;
}

.mp-list .mpl-features{
  height:36rpx;
  font-size:0;
  margin-top:10rpx;
  margin-right:54rpx;
}

.mp-list.no-btns .mpl-features{
  margin-right:0;
}

.mp-list .mpl-features>view{
  max-width:32vw;
  font-size:24rpx;
  color:#fff;
  display:inline-block;
  height:100%;
  line-height:36rpx;
  padding-left:12rpx;
  padding-right:12rpx;
  background-image: linear-gradient(43deg, #FF8522 0%, #FFAA4E 100%);
  border-radius: 4rpx;
  margin-right:16rpx;
  box-sizing:border-box;
  vertical-align:top;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.mp-list .mpl-features>view:last-child{
  margin-right:0;
}

.mp-list .mpl-features>view.mpl-sales{
  background-image: linear-gradient(43deg, #FF393C 0%, #FF7558 100%);
}

.mp-list .mpl-info{
  display:flex;
  align-items:flex-end;
  margin-top:18rpx;
}

.mp-list .mpl-info>view:last-child{
  margin-right:0;
}

.mp-list .mpl-save{
  font-size:24rpx;
  margin-right:12rpx;
  color:#F2270B;
}

.mp-list .mpl-save>view{
  font-size:40rpx;
  font-family: 'TTTGB';
  display:inline-block;
  vertical-align:bottom;
  line-height:1.2;
}

.mp-list .mpl-text{
  font-size:24rpx;
  color:#8C9094;
  margin-right:8rpx;
  vertical-align:bottom;
  line-height:1.2;
}

.mp-list .mpl-text.disabled{
  text-decoration:line-through;
}

.mp-list .mpl-img{
  width:192rpx;
  height:192rpx;
  max-width:96px;
  max-height:96px;
}

.mp-list .mpl-img>image{
  width:100%;
  height:100%;
  border-radius:5px;
}

@media screen and (max-width: 340px){
  .mp-list .mpl-features>view{
    max-width:32.8vw;
  }
}

@media screen and (min-width: 375px) {
  .mp-list .mpl-item{
    margin-bottom:28px;
  }
  .mp-list .mpl-info{
    margin-top:9px;
  }
  .mp-list .mpl-title{
    font-size:14px;
    line-height:20px;
    height:40px;
    margin-right:35px;
  }
  .mp-list.no-btns .mpl-title{
    margin-right:0;
  }
  .mp-list .mpl-tag{
    height:16px;
    line-height:16px;
    font-size:11px;
    padding-left:5px;
    padding-right:5px;
    margin-right:2px;
    border-radius:2px;
  }
  .mp-list .mpl-features{
    height:18px;
    margin-top:5px;
    margin-right:27px;
  }
  .mp-list.no-btns .mpl-features{
    margin-right:0;
  }
  .mp-list .mpl-features>view{
    font-size:12px;
    line-height:18px;
    padding-left:6px;
    padding-right:6px;
    border-radius:2px;
    margin-right:8px;
  }
  .mp-list .mpl-save{
    font-size:12px;
    margin-right:13px;
  }
  .mp-list .mpl-save>view{
    font-size:20px;
  }
  .mp-list .mpl-text{
    font-size:12px;
    margin-right:8px;
  }
}